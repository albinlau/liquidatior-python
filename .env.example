# 网络 RPC
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc

# 部署账户私钥 (不要带 0x 前缀)
PRIVATE_KEY=

# Arbiscan API Key (用于合约验证)
ETHERSCAN_API_KEY=

# Gas 配置 (可选)
GAS_PRICE=100000000 # 0.1 Gwei
GAS_LIMIT=5000000   # 500万

# 监控配置 (用于监控脚本)
MONITOR_INTERVAL=1000           # 监控间隔(毫秒)
MIN_HEALTH_FACTOR=1.0          # 最小健康因子
MIN_LIQUIDATION_VALUE=1000     # 最小清算价值(USD)
MAX_GAS_PRICE=150000000        # 最大 Gas 价格(0.15 Gwei)

# 合约配置
MIN_PROFIT_THRESHOLD=0.1
MAX_SLIPPAGE=100 